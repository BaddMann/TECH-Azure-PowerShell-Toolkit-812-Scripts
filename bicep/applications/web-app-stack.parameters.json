{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "myapp"
    },
    "environment": {
      "value": "dev"
    },
    "location": {
      "value": "East US 2"
    },
    "administratorObjectId": {
      "value": "YOUR_AZURE_AD_OBJECT_ID_HERE"
    },
    "dbAdminUsername": {
      "value": "sqladmin"
    },
    "dbAdminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/YOUR_SUBSCRIPTION_ID/resourceGroups/YOUR_KEYVAULT_RG/providers/Microsoft.KeyVault/vaults/YOUR_KEYVAULT_NAME"
        },
        "secretName": "sql-admin-password"
      }
    },
    "logAnalyticsWorkspaceId": {
      "value": ""
    },
    "enableApplicationGateway": {
      "value": false
    },
    "enablePrivateEndpoints": {
      "value": false
    },
    "customDomainName": {
      "value": ""
    },
    "tags": {
      "value": {
        "Environment": "Development",
        "Application": "MyWebApp",
        "Owner": "Development Team",
        "CostCenter": "IT-DEV-001",
        "Project": "WebApp-Modernization",
        "DataClassification": "Internal",
        "BusinessUnit": "Technology",
        "SupportContact": "devteam@company.com",
        "BackupRequired": "Yes",
        "MaintenanceWindow": "Sunday 02:00-04:00 UTC"
      }
    }
  }
}
